{
    "corner": {
        "-c": {
            "label": "Corner detector",
            "style": "combobox",
            "entries": ["Rohr", "Tomasi", "Foerstner"],
            "default": "Rohr"
        },
        "-q": {
            "label": "Percentile",
            "style": "slider",
            "limits": [
                0,
                1,
                0.01,
                0.1
            ]
        },
        "-s": {
            "label": "Noisescale [sigma]",
            "style": "slider",
            "limits": [
                0,
                10,
                0.1,
                0.5
            ]
        },
        "-r": {
            "label": "Integration scale [rho]",
            "style": "slider",
            "limits": [
                0,
                10,
                0.1,
                0.5
            ]
        },
        "-m": {
            "label": "Mask radius",
            "style": "slider",
            "limits": [
                0,
                20,
                1,
                4
            ]
        },
        "-C": {
            "label": "Circular suppression",
            "style": "checkbox",
            "default": true
        },
        "-P": {
            "label": "Total pixel percentile",
            "style": "checkbox",
            "default": true
        },
        "-D": {
            "label": "Use sharper kernel",
            "style": "checkbox",
            "default": true
        }
    },
    "inpaint": {
        "-s": {
            "style": "slider",
            "label": "Noisescale [sigma]",
            "limits": [
                0,
                10,
                0.1,
                1
            ]
        },
        "-l": {
            "style": "slider",
            "label": "Contrast [lambda]",
            "limits": [
                0,
                1,
                0.01,
                0.8
            ]
        },
        "-a": {
            "style": "slider",
            "label": "Dissipativity [alpha]",
            "limits": [
                0,
                0.5,
                0.01,
                0.49
            ]
        },
        "-g": {
            "style": "slider",
            "label": "Nonnegativity [gamma]",
            "limits": [
                0,
                1,
                0.01,
                1
            ]
        },
        "-n": {
            "style": "textbox",
            "label": "Solver iterations",
            "default": 200
        }
    }
}
